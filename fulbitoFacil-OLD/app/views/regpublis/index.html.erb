<% @regpublis.each do |regpubli| %>
  <tr>
    <td><%= regpubli.Titulo %></td>
    <td><%= regpubli.Fecha_Inicio %></td>
    <td><%= regpubli.Fecha_fin %></td>
    <td><%= regpubli.Tarifa %></td>
    <td><%= regpubli.Num_Click %></td>
    <td><%= regpubli.Ubicacion %></td>
    <td><%= regpubli.Contenido %></td>
    <td><%= link_to 'Show', regpubli %></td>
    <td><%= link_to 'Edit', edit_regpubli_path(regpubli) %></td>
    <td><%= link_to 'Destroy', regpubli, confirm: 'Are you sure?', method: :delete %></td>
